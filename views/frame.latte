<!DOCTYPE html>
<head>
    <link href="{$config['favicon_url']}" rel="SHORTCUT ICON">
    <meta charset="UTF-8">
    <meta name="author" content="PRASEOD-">
    <meta name="title" content="{$config['sitename']}">
    <meta name="description" content="{$config['description']}">
    <meta http-equiv="Content-Type" content="text/html;">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta property="og:type" content="website">
    <meta property="og:title" content="{$config['sitename']}">
    <meta property="og:description" content="{$config['description']}?>">
    <script>apiuri = '/internal/preview'</script>
    <link rel="stylesheet" href="/src/style/base.css">
    <script defer src="/src/script/main.js"></script>
    <title> {$wiki['page']['title']} - {$config['sitename']} </title>
    {if $wiki['page']['view_name'] == 'wiki'}
        <link rel="stylesheet" href="/src/style/namumark.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/katex@0.11.1/dist/katex.min.css" integrity="sha384-zB1R0rpPzHqg7Kpt0Aljp8JPLqbXI3bhnPWROx27a9N0Ll6ZP/+DiW/UqRcLbRjq" crossorigin="anonymous"/>
        <script defer src="https://cdn.jsdelivr.net/npm/katex@0.11.1/dist/katex.min.js" integrity="sha384-y23I5Q6l+B6vatafAwxRu/0oK/79VlbSz7Q9aiSZUvyWYIYsd+qj+o24G5ZU2zJz" crossorigin="anonymous"></script>
        <script defer src="https://cdn.jsdelivr.net/npm/katex@0.11.1/dist/contrib/auto-render.min.js" integrity="sha384-kWPLUVMOks5AQFrykwIup5lo0m3iMkkHrD0uJ4H5cjeGihAutqP0yW0J6dpFiVkI" crossorigin="anonymous" onload="renderMathInElement(document.body);"></script>
        <script defer src="/src/script/namumark.js"></script>
    {/if}
    {if $wiki['page']['view_name'] == 'edit'}
        <script>
        monacoVersion = {$config['editor_version']};
        DocContent = {json_encode(['c' => $wiki['page']['data']['editor']['raw']], JSON_UNESCAPED_UNICODE)};
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/monaco-editor/{$config['editor_version']}/min/vs/loader.js"></script>
        <script src="/src/script/monaco.js"></script>
    {/if}
    {foreach $skinConfig['auto_css_targets']['*'] as $sf}
        <link rel="preload" as="style" href="/skins/{$skinName}/{$sf|noCheck}">
        <link rel="stylesheet" href="/skins/{$skinName}/{$sf|noCheck}">
    {/foreach}
    {foreach $skinConfig['auto_js_targets']['*'] as $sf}
        <link rel="preload" as="script" href="/skins/{$skinName}/{$sf['path']|noCheck}">
        <script defer src="/skins/{$skinName}/{$sf['path']|noCheck}"></script>
    {/foreach}
    {$skinConfig['additional_heads']|implode|noescape}
</head>
<body>
    <div id="app">
        <div class="{$skinConfig['body_classes']|implode:' '}">
            {$body|noescape}
        </div>
    </div>
</body>